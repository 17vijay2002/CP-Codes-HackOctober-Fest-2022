#include <bits/stdc++.h>

using namespace std;

int main()
{
    string a,b;
    int c,d,r=0,s=0;
    cin>>a>>b;
    c=a.length();
    d=b.length();
    for(int x=0;x<c;x++){
        int p = a[x];
        a[x]=tolower(p);
        int q = b[x];
        b[x]=tolower(q);
        r = a[x] +r;
        s = b[x] +s;
    if(r!=s){   
            if (r>s){cout<<1;}
            else if(s>r){cout<<-1;}
            return 0;
            }
    
    } 
    cout<<0;
    
    return 0;
}
    
